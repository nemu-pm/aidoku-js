{
  "name": "aidoku-js",
  "version": "0.1.0",
  "private": true,
  "description": "Run Aidoku sources in JavaScript environments",
  "repository": {
    "type": "git",
    "url": "https://github.com/nemu-pm/aidoku-js.git"
  },
  "homepage": "https://github.com/nemu-pm/aidoku-js#readme",
  "scripts": {
    "typecheck": "bun run typecheck:runtime && bun run typecheck:cli",
    "typecheck:runtime": "bunx tsc --noEmit -p runtime",
    "typecheck:cli": "bunx tsc --noEmit -p cli",
    "test:e2e:agent": "bun tests/e2e-agent-source.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
